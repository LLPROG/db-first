classe	            SMALLINT		                                        NULL
cavalli	            SMALLINT		                                        NULL
potenza	            SMALLINT		                                        NULL
cambio 	            manuale: 0, automatico: 1 SMALLINT		                NOTNULL
marce	            SMALLINT		                                        NOTNULL
trazione	        anteriore: 0 , posteriore: 1 SMALLINT		            NULL
			
